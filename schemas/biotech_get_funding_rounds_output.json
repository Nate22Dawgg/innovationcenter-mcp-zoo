{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Biotech Get Funding Rounds Output",
  "description": "Output schema for company funding rounds",
  "type": "object",
  "properties": {
    "company_name": {
      "type": "string",
      "description": "Company name"
    },
    "funding_rounds": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "description": "Funding round type"
          },
          "filing_date": {
            "type": "string",
            "description": "Filing date"
          },
          "form_type": {
            "type": "string",
            "description": "SEC form type"
          },
          "accession_number": {
            "type": "string",
            "description": "SEC accession number"
          },
          "note": {
            "type": "string",
            "description": "Additional notes"
          }
        }
      },
      "description": "List of funding rounds"
    },
    "data_source": {
      "type": "string",
      "description": "Data source"
    },
    "limitation": {
      "type": "string",
      "description": "Data limitations note"
    },
    "count": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of funding rounds"
    }
  },
  "required": ["company_name", "funding_rounds", "count"],
  "additionalProperties": false
}

