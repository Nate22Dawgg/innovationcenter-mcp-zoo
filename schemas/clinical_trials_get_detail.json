{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Clinical Trials Get Detail Input",
  "description": "Input schema for retrieving detailed clinical trial information",
  "type": "object",
  "properties": {
    "nct_id": {
      "type": "string",
      "pattern": "^NCT[0-9]{8}$",
      "description": "ClinicalTrials.gov identifier (e.g., NCT01234567)",
      "examples": ["NCT01234567", "NCT04567890"]
    }
  },
  "required": ["nct_id"],
  "additionalProperties": false
}

