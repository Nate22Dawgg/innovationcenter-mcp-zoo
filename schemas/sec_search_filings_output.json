{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SEC Search Filings Output",
  "description": "Output schema for filing search results",
  "type": "object",
  "properties": {
    "keyword": {
      "type": "string",
      "description": "Search keyword used"
    },
    "form_type": {
      "type": ["string", "null"],
      "description": "Form type filter applied"
    },
    "count": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of filings found"
    },
    "filings": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "form_type": {
            "type": "string"
          },
          "filing_date": {
            "type": "string"
          },
          "accession_number": {
            "type": "string"
          },
          "cik": {
            "type": "string"
          },
          "company_name": {
            "type": "string"
          }
        },
        "additionalProperties": true
      }
    },
    "error": {
      "type": "string",
      "description": "Error message if search failed"
    }
  },
  "additionalProperties": false
}

