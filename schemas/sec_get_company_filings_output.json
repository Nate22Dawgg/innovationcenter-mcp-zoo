{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SEC Get Company Filings Output",
  "description": "Output schema for company filings",
  "type": "object",
  "properties": {
    "cik": {
      "type": "string",
      "description": "Company CIK"
    },
    "company_name": {
      "type": ["string", "null"],
      "description": "Company name"
    },
    "form_type": {
      "type": ["string", "null"],
      "description": "Form type filter applied"
    },
    "count": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of filings returned"
    },
    "filings": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "form_type": {
            "type": "string",
            "description": "Form type (e.g., '10-K', '10-Q')"
          },
          "filing_date": {
            "type": "string",
            "description": "Filing date (YYYY-MM-DD)"
          },
          "report_date": {
            "type": "string",
            "description": "Report date (YYYY-MM-DD)"
          },
          "description": {
            "type": "string",
            "description": "Filing description"
          },
          "accession_number": {
            "type": "string",
            "description": "Filing accession number"
          },
          "file_number": {
            "type": "string",
            "description": "File number"
          },
          "primary_document": {
            "type": "string",
            "description": "Primary document filename"
          },
          "cik": {
            "type": "string",
            "description": "Company CIK"
          }
        },
        "additionalProperties": false
      }
    },
    "error": {
      "type": "string",
      "description": "Error message if request failed"
    }
  },
  "additionalProperties": false
}

